{% block content %}
  {% include "navigation.jinja2" %}

    <div class="row">
      <h1>{{ title }}</h1>

      <div class="card border-primary mb-3" style="max-width: 50%;" >
        <div class="card-header">{{gauge_addr}}</div>
        <div class="card-body">
          <h4 class="card-title"></h4>

        </div>
      </div>
      
      <div class="card border-secondary mb-3" style="max-width: 50%;" >
        <div class="card-header">Something went wrong</div>
        <div class="card-body">
          <h4 class="card-title text-primary">No Votes Found</h4>
          <p class="card-text">It does not appear that this gauge has been voted into governance yet.</p> 

        </div>
      </div>
    </div>



{% endblock %}
